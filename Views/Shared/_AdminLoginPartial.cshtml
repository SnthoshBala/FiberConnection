@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor
<ul class="navbar-nav">
    @{
        ViewBag.Data = HttpContextAccessor.HttpContext.Session.GetString("Username");
    }
@if (@ViewBag.Data!=null)
{
    <li class="nav-item">
        <a class="nav-link text-dark" asp-controller="Admin" asp-action="AdminControl" >@ViewBag.Data</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-controller="Admin" asp-action="Logout">Logout</a>
    </li>
}
</ul>
